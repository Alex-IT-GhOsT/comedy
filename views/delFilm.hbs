<form method="post" action="/delFilmSucces">
    <div>
        <span>Выбрать фильм для удаления: </span>
        <select name="film" >
            {{#each films}}
            <option value={{_id}}>{{title}}</option>
            {{/each}}
        </select>   
        <button type="submit" name="submit">Удалить</button>
    </div>
</form>